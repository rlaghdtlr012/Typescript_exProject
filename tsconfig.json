{
    "compilerOptions": {
        "allowJs": true,
        "target": "ES5",
        "outDir": "./built",
        "moduleResolution": "Node",
        "lib": ["ES2015", "DOM", "DOM.Iterable"],
        "noImplicitAny": true, // type에 대해서 지정을 안해줬다면, 일단 any라도 지정을 해줘라라는 옵션
    },
    "include": ["./src/**/*"]
}